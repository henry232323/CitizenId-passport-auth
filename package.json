{
  "name": "@citizenid/passport-citizenid",
  "version": "1.0.0",
  "description": "Passport strategy for authenticating with Citizen iD using OAuth2 and OpenID Connect",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "files": [
    "lib",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "eslint": "eslint . --cache --cache-location .cache/eslint",
    "eslint-fix": "eslint --cache --cache-location .cache/eslint --fix .",
    "lint": "run-s eslint markdown-lint",
    "lint-fix": "run-s eslint-fix markdown-lint-fix",
    "markdown-lint": "markdownlint-cli2",
    "markdown-lint-fix": "markdownlint-cli2 --fix",
    "prepare": "run-s type-check",
    "prepublishOnly": "run-s lint build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "passport",
    "citizenid",
    "oauth2",
    "oidc",
    "openid",
    "authentication",
    "auth",
    "strategy"
  ],
  "author": "Arkanis Corporation",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArkanisCorporation/CitizenId-passport-auth.git"
  },
  "bugs": {
    "url": "https://github.com/ArkanisCorporation/CitizenId-passport-auth/issues"
  },
  "homepage": "https://github.com/ArkanisCorporation/CitizenId-passport-auth#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest",
    "access": "public"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.3",
    "passport-oauth2": "^1.8.0"
  },
  "peerDependencies": {
    "passport": "^0.6.0 || ^0.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/passport": "^1.0.16",
    "@types/passport-oauth2": "^1.4.15",
    "eslint": "^9.39.2",
    "markdownlint-cli2": "^0.20.0",
    "markdownlint-cli2-formatter-template": "^0.0.4",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "engines": {
    "node": ">= 14.0.0"
  }
}
